@model IEnumerable<MohInpatient.Models.Inpatient>

@{
    ViewData["Title"] = "Index";
}


<p>
    <a class="btn btn-success" asp-action="Create">Create New</a>
</p>
<table class="table table-bordered">
    <thead>
        <tr>
           <th>
                @Html.DisplayNameFor(model => model.DohCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FacName)
            </th>
           <th>
                @Html.DisplayNameFor(model => model.FPaName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SPaName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TPaName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FoPaName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FMoName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SMoName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TMoName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
           <td>
                    @Html.DisplayFor(modelItem => item.DohCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FacName)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.FPaName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SPaName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TPaName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FoPaName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FMoName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SMoName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TMoName)
            </td>
           
            <td>
                    <a class="btn btn-info"  asp-action="Edit" asp-route-id="@item.InpatientId">Edit</a> |
                    <a class="btn btn-warning"  asp-action="Details" asp-route-id="@item.InpatientId">Details</a> |
                    <a class="btn btn-danger"  asp-action="Delete" asp-route-id="@item.InpatientId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
